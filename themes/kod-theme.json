{
    "name": "My Custom Theme",
    "type": "dark",
    "colors": {
        "editor.background": "#222222",
        "editor.foreground": "#eeeeee",
        "editor.selectionBackground": "#2F3F52",
        "editor.lineHighlightBackground": "#333333",
        "editorCursor.foreground": "#ff3319",
        "editorWhitespace.foreground": "#444444",
        "editorIndentGuide.activeBackground": "#444444",
        "editorIndentGuide.background": "#333333"
    },
    "tokenColors": [
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#66c8ef"
            }
        },
        {
            "scope": "type",
            "settings": {
                "foreground": "lightsalmon"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#CAC059"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#C969B6"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#9aca7e",
                "background": "#212A24"
            }
        },
        {
            "scope": "constant.character",
            "settings": {
                "foreground": "#C0D164",
                "background": "#2B2F26"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#FFB14B",
                "background": "#342C22"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#666666"
            }
        },
        {
            "scope": "support.function",
            "settings": {
                "foreground": "#85FFDF",
                "background": "#1F2B31"
            }
        },
        {
            "scope": "punctuation",
            "settings": {
                "foreground": "#ddd"
            }
        },
        {
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#946B57"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "foreground": "#77B5FF"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "foreground": "#F09C9F"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "foreground": "#A78AB0"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#8CB194"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#cda869"
            }
        },
        {
            "scope": "markup.list",
            "settings": {
                "foreground": "#3C7E5B"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#bbeecc",
                "background": "#2c2c2c"
            }
        },
        {
            "scope": "constant.other.color",
            "settings": {
                "foreground": "orange"
            }
        },
        {
            "scope": "constant.other.symbol",
            "settings": {
                "foreground": "#8D86EE"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#f7bfb6",
                "background": "#42201C"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#cff7bf",
                "background": "#13340C"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#3D96DE",
                "background": "#3D96DE"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#f9ed97"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "foreground": "#ffaa22"
            }
        }
    ]
}
